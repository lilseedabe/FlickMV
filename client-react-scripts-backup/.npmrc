# ===========================
# FlickMV Client .npmrc
# 依存関係解決とビルド問題の修正
# ===========================

# 厳密な依存関係解決
legacy-peer-deps=true
strict-peer-deps=false

# パッケージロック設定
package-lock=true
package-lock-only=false

# キャッシュ設定
cache-min=86400

# レジストリ設定
registry=https://registry.npmjs.org/

# インストール設定
prefer-online=true
audit-level=moderate

# 脆弱性チェック
audit=true